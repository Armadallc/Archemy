# Full Calendar Integration Feature Flags
# Local development overrides - gitignored
# Set to 'true' to enable individual features
# Note: Vite requires VITE_ prefix for client-side variables

# View Features
VITE_ENABLE_FULL_CALENDAR_VIEWS=false
VITE_ENABLE_MONTH_VIEW=true
VITE_ENABLE_YEAR_VIEW=false
VITE_ENABLE_AGENDA_VIEW=true
VITE_ENABLE_FULL_CALENDAR_LAYOUT=true
# Interaction Features
VITE_ENABLE_EVENT_RESIZE=true
VITE_ENABLE_TIME_FORMAT=true
VITE_ENABLE_STAFF_FILTER=true
