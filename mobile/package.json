{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 8082",
    "android": "expo start --android --port 8082",
    "ios": "expo start --ios --port 8082",
    "web": "expo start --web --port 8082 --lan",
    "web:8082": "expo start --web --port 8082 --lan",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:web:prod": "EXPO_PUBLIC_API_URL=https://halcyon-backend.onrender.com EXPO_PUBLIC_WS_URL=wss://halcyon-backend.onrender.com expo export --platform web --output-dir web-build",
    "dev": "expo start --ios --port 8082",
    "dev:clear": "expo start --clear --ios --port 8082",
    "dev:web": "expo start --web --port 8082 --lan",
    "reset": "rm -rf .expo node_modules/.cache && expo start --clear --port 8082",
    "eas:login": "npx eas-cli@latest login",
    "eas:init": "npx eas-cli@latest init",
    "eas:build:ios:dev": "npx eas-cli@latest build --platform ios --profile development",
    "eas:build:ios:preview": "npx eas-cli@latest build --platform ios --profile preview",
    "eas:build:ios:prod": "npx eas-cli@latest build --platform ios --profile production",
    "eas:submit": "npx eas-cli@latest submit --platform ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.4.9",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~54.0.30",
    "expo-blur": "~15.0.8",
    "expo-camera": "~17.0.10",
    "expo-constants": "~18.0.12",
    "expo-device": "~8.0.10",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-location": "~19.0.8",
    "expo-notifications": "~0.32.15",
    "expo-router": "~6.0.21",
    "expo-secure-store": "~15.0.8",
    "expo-sensors": "~15.0.8",
    "expo-status-bar": "~3.0.9",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.5.14",
    "@types/react": "~19.1.10",
    "jest": "~29.7.0",
    "jest-expo": "~54.0.16",
    "typescript": "~5.9.2"
  },
  "private": true
}
