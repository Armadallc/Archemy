{
  "name": "mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --port 8082",
    "android": "expo start --android --port 8082",
    "ios": "expo start --ios --port 8082",
    "web": "expo start --web --port 8082",
    "web:8082": "expo start --web --port 8082",
    "build:web": "expo export --platform web --output-dir web-build",
    "build:web:prod": "EXPO_PUBLIC_API_URL=https://halcyon-backend.onrender.com EXPO_PUBLIC_WS_URL=wss://halcyon-backend.onrender.com expo export --platform web --output-dir web-build",
    "dev": "expo start --ios --port 8082",
    "dev:clear": "expo start --clear --ios --port 8082",
    "dev:web": "expo start --web --port 8082",
    "reset": "rm -rf .expo node_modules/.cache && expo start --clear --port 8082"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.2",
    "@react-navigation/native": "^7.1.8",
    "@react-navigation/stack": "^7.4.9",
    "@tanstack/react-query": "^5.90.2",
    "expo": "~54.0.12",
    "expo-camera": "^17.0.8",
    "expo-constants": "^18.0.9",
    "expo-device": "^8.0.9",
    "expo-image-picker": "^17.0.8",
    "expo-linking": "^8.0.8",
    "expo-location": "^19.0.7",
    "expo-notifications": "^0.32.12",
    "expo-router": "^6.0.10",
    "expo-secure-store": "^15.0.7",
    "expo-sensors": "^15.0.7",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.1"
  },
  "devDependencies": {
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^30.0.0",
    "@types/react": "~19.1.10",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "typescript": "~5.9.2"
  },
  "private": true
}
